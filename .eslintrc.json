{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "react"
  ],
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "rules": {
    "indent": [
      2,
      2
    ],
    "quotes": [
      2,
      "single"
    ],
    "comma-dangle": [
      1,
      "always-multiline"
    ],
    "semi": 2,
    "no-console": 0,
    "no-undef": 2,
    "react/prop-types": 0,
    "react/react-in-jsx-scope": 0
  }
}